.new{
  width: 100%;
  height: 70rpx;
  background: #f2f2f2;
  text-align: center;
  line-height: 70rpx;
  color: #6bc855;
}

.pro-item{
  overflow: hidden;
  padding: 30rpx;
}
.pro-logo{
  float: left;
  width: 190rpx;
  height: 190rpx;
}
.pro-body{
  margin-left: 210rpx;
}
.pro-title{
  color: #212121;
  font-size: 40rpx;
  line-height: 1;
}
.pro-desc{
  font-size: 30rpx;
  color: #9a9a0a;
  font-family: 'Courier New', Courier, monospace;
  line-height: 1;
}
.pro-footer{
  overflow: hidden;
 
}
.btn-detail{
  float: left;
  width: 170rpx;
  height: 52rpx;
}
.btn-ask{
  float: left;
  width: 224rpx;
  height: 52rpx;
  margin-left: 20rpx;
  padding: 0;
  line-height:1;
}
.btn-ask image{
  width: 100%;
  height: 100%;
}

